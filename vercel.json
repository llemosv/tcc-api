{
  "version": 2,
  "builds": [
      {
          "src": "src/main.ts",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/modules/(.*)",
          "dest": "src/main.ts",
          "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE",
            "PATCH"
          ]
      }
  ],
  "outputDirectory": "public"
}